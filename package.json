{
  "name": "moth-dev",
  "displayName": "Moth Dev",
  "description": "VS Code support for the Moth language.",
  "repository": "https://github.com/StellarWitch7/moth-lang",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "moth",
        "aliases": [
          "Moth",
          "moth"
        ],
        "extensions": [
          "moth"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "moth",
        "scopeName": "source.moth",
        "path": "./syntaxes/moth.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "@vscode/vsce": "^2.22.0",
    "vsce": "^2.15.0"
  }
}
